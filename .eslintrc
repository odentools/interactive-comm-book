
{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": 0,
        "no-console": 0,
        "angular/di": 0,
        "angular/log": 0,
        "angular/controller-as": 0,
        "angular/controller-as-route": 0,
        "angular/document-service": 0,
        "angular/controller-name": [2, "/[A-Z].*Ctrl$/"],
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "angular"
    ]
}
